    {
        "id": "algangror_1",
        "message": "Oh my, a child. He he, how nice. Tell me, what brings you here?",
        "rewards": [
            {
                "rewardType": "questProgress",
                "rewardID": "algangror",
                "value": 10
            }
        ],
        "replies": [
            {
                "text": "I am looking for my brother.",
                "nextPhraseID": "algangror_2a"
            },
            {
                "text": "I just entered to see if there's any loot to be found here.",
                "nextPhraseID": "algangror_2b"
            },
            {
                "text": "I'm an adventurer, looking to help anyone in need of help.",
                "nextPhraseID": "algangror_2c"
            },
            {
                "text": "I'd rather not tell.",
                "nextPhraseID": "algangror_2d"
            },
            {
                "text": "I am sent by Jhaeld to end whatever it is you do to the people of Remgard.",
                "nextPhraseID": "algangror_fight_1",
                "requires": [
                    {
                        "requireType": "questProgress",
                        "requireID": "remgard2",
                        "value": "21"
                    }
                ]
            }
        ]
    },
    {
        "id": "algangror_2a",
        "message": "Run away, has he? He he.",
        "replies": [
